{
    "background": {
	"scripts": [ "jquery-min.js", "settings.js", "background.js" ]
    },
    "browser_action": {
	"default_icon": "img/emacs.png",
	"default_title": "ChromEmacs",
	"default_popup": "popup.html"
    },
    "content_scripts": [ {
	"all_frames": true,
	"css": [ "emacs.css" ],
	"js": [ "jquery-min.js", "keys.js", "emacs.js" ],
	"matches": [ "\u003Call_urls\u003E" ],
	"run_at": "document_start"
    } ],
    "description": "emacs key bindings for chromium",
    "icons": {
	"128": "img/icon128.png",
	"16": "img/icon16.png",
	"48": "img/icon48.png"
    },
    "name": "ChromEmacs",
    "options_page": "options.html",
    "permissions": [ "tabs", "bookmarks", "clipboardRead", "\u003Call_urls\u003E" ],
    "version": "1.1"
}
